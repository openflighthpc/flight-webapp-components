{
  "presets": [
    [
      "@babel/preset-env", { "modules": false }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-proposal-object-rest-spread",
    "@babel/plugin-proposal-class-properties",
    [
      "css-modules-transform", {
        "extractCss": "./dist/styles/flight-webapp-components.css"
      }
    ]
  ]
}
